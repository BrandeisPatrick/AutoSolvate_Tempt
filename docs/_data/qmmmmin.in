gpr QMMM heat
 &cntrl
  imin   = 0,
  irest  = 0, ! 0- new simulation 1- restart
  ntx    = 1, ! 1-read in coordinates, but not velocity, 5-both
  cut    = 8.0,
  ig     = -1, !random seed
  ntc    = 2, ntf    = 2, !Shake is used for solvent
  ntb    = 1,
  tempi  = 0,
  temp0  = 0,
  ntt    = 3, ! 1=rescale 3=Langevin dynamics 7-bussi
  vrand  = 1,
  tautp  = 0.01,
  gamma_ln = 500.0, !Langevin dynamics collision frequency
  nstlim = 8,  !num steps
  dt = 0.0001,  !in ps
  ntpr   = 1, !print detials to log every step
  ntwx   = 1, !write coordinates to mdcrd every step
  ntwr   = 1, !write restart file every step
  ifqnt  = 1,!turn on QM/MM
/
 &qmmm
  qmmask    = ':1',
  qmcut     = 8,
  qmcharge  = 0,
  qmshake   = 0, !Shake QM H atoms if shake is turned on (NTC>1) (default)
  qm_theory = 'EXTERN',
  qm_ewald  = 0,
  qmgb      = 0,
  verbosity = 2,
  writepdb  = 1,
 /
 &tc
  executable = 'terachem',
  use_template = 1,
 /
