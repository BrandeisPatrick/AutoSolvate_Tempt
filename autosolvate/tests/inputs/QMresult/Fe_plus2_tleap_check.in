source oldff/leaprc.ff14SB
source leaprc.gaff
source leaprc.water.opc
addAtomTypes {
        { "M1"  "Fe" "sp3" }
        { "Y7"  "N" "sp3" }
        { "Y8"  "N" "sp3" }
        { "Y9"  "N" "sp3" }
        { "Z1"  "N" "sp3" }
        { "Z2"  "N" "sp3" }
        { "Z3"  "N" "sp3" }
}
FE1 = loadmol2 FE1.mol2
L01 = loadmol2 L01.mol2
L11 = loadmol2 L11.mol2
L21 = loadmol2 L21.mol2
loadamberparams LG0.frcmod
loadamberparams LG1.frcmod
loadamberparams LG2.frcmod
loadamberparams frcmod.ionslm_126_opc
loadamberparams Fe_plus2_mcpbpy.frcmod
mol = loadpdb Fe_plus2_mcpbpy.pdb
bond mol.1.FE mol.2.N
bond mol.1.FE mol.2.N1
bond mol.1.FE mol.3.N
bond mol.1.FE mol.3.N1
bond mol.1.FE mol.4.N
bond mol.1.FE mol.4.N1
savepdb mol Fe_plus2_dry.pdb
saveamberparm mol Fe_plus2_dry.prmtop Fe_plus2_dry.inpcrd
solvatebox mol OPCBOX 10.0
addions mol Na+ 0
addions mol Cl- 0
savepdb mol Fe_plus2_solv.pdb
saveamberparm mol Fe_plus2_solv.prmtop Fe_plus2_solv.inpcrd
quit
 
