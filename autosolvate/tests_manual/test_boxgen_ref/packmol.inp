# All atoms from diferent molecules will be at least 2.0 Angstroms apart
tolerance 2

filetype pdb

output ch3cn_solvated.packmol.pdb

# add the solute
structure solute.pdb
   number 1
   fixed  27.0 27.0 27.0 0. 0. 0.
   centerofmass
   resnumbers 2 
end structure

# add first type of solvent molecules
structure ch3cn.pdb
  number 1680 
  inside cube 0. 0. 0. 54 
  resnumbers 2 
end structure
